   ]
  },
  {
   "cell_type": "code",
   "execution_count": 84,
   "metadata": {},
   "outputs": [
    {
     "data": {
      "text/plain": [
       "4.170800237107298"
      ]
     },
     "execution_count": 84,
     "metadata": {},
