   "metadata": {},
   "outputs": [],
   "source": [
    "#removal of $ sign\n",
    "price = []                                    \n",
    "for i in df['Price']:               \n",
    "    if i[0]=='$':                    \n",
    "        price.append(i[1:])\n",
    "    else:\n",
    "        price.append(i)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 24,
   "metadata": {},
   "outputs": [],
